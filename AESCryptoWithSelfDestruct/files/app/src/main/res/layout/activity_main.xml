<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/buttonEraseAllatStart1"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="delete all"
            android:textColor="#FFFFFF" />

        <Button
            android:id="@+id/buttonEraseAllatStart2"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="delete all"
            android:textColor="#FFFFFF" />

        <Button
            android:id="@+id/buttonEraseAllatStart3"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="delete all"
            android:textColor="#FFFFFF" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/textViewPassInfo"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="Create password"
            android:textColor="#FFFFFF"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <TextView
            android:id="@+id/textViewPass1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="○"
            android:textColor="#FFFFFF"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textViewPass2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="○"
            android:textColor="#FFFFFF"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textViewPass3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="○"
            android:textColor="#FFFFFF"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textViewPass4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="○"
            android:textColor="#FFFFFF"
            android:textSize="24sp" />

        <Button
            android:id="@+id/buttonPassDel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="DEL"
            android:textColor="#FFFFFF"
            android:textSize="24sp" />

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <Button
            android:id="@+id/buttonPass1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="1"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass4"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonPass2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="2"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass5"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass3"
            app:layout_constraintStart_toEndOf="@+id/buttonPass1"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonPass3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="3"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonPass2"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonPass4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="4"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass7"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass1" />

        <Button
            android:id="@+id/buttonPass5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="5"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass8"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass6"
            app:layout_constraintStart_toEndOf="@+id/buttonPass4"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass2" />

        <Button
            android:id="@+id/buttonPass6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="6"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPass9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonPass5"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass3" />

        <Button
            android:id="@+id/buttonPass7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="7"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPassOk"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass4" />

        <Button
            android:id="@+id/buttonPass8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="8"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPassOk"
            app:layout_constraintEnd_toStartOf="@+id/buttonPass9"
            app:layout_constraintStart_toEndOf="@+id/buttonPass7"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass5" />

        <Button
            android:id="@+id/buttonPass9"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="9"
            android:textColor="#ffffff"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonPassOk"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonPass8"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass6" />

        <Button
            android:id="@+id/buttonPassOk"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#000000"
            android:fontFamily="sans-serif-light"
            android:text="OK"
            android:textColor="#ffffff"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonPass7" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>