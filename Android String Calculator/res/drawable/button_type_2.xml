<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true" >
    <shape>
        <gradient
            android:startColor="#333333"
            android:endColor="#303030"
            android:angle="270" />
        <stroke
            android:width="1dp"
            android:color="#303030" />
        <corners
            android:radius="14dp" />
        <padding
            android:left="0dp"
            android:top="0dp"
            android:right="0dp"
            android:bottom="0dp" />
    </shape>
</item>
<item>
    <shape>
        <gradient
            android:startColor="#ffa11d"
            android:endColor="#d18418"
            android:angle="270" />
        <stroke
            android:width="1dp"
            android:color="#d18418" />
        <corners
            android:radius="14dp" />
        <padding
            android:left="0dp"
            android:top="0dp"
            android:right="0dp"
            android:bottom="0dp" />
    </shape>
</item>
</selector>